<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mashibing.mapper.TblArgRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mashibing.bean.TblArgRecord">
        <id column="arg_code" property="argCode" />
        <result column="arg_name" property="argName" />
        <result column="arg_value" property="argValue" />
        <result column="arg_desc" property="argDesc" />
        <result column="arg_order" property="argOrder" />
        <result column="belong_product" property="belongProduct" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        arg_code, arg_name, arg_value, arg_desc, arg_order, belong_product
    </sql>

</mapper>
