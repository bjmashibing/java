        <result column="send_tag" property="sendTag" />
        <result column="timing_date" property="timingDate" />
        <result column="message_type" property="messageType" />
        <result column="extend_phone" property="extendPhone" />
        <result column="receive_phone" property="receivePhone" />
        <result column="message_content" property="messageContent" />
        <result column="is_send" property="isSend" />
        <result column="receive_identify" property="receiveIdentify" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, send_name, request_date, send_tag, timing_date, message_type, extend_phone, receive_phone, message_content, is_send, receive_identify
